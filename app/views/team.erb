
<h1>Team Name: <%= @team.name %></h1>
<h2>Team Motto: <%= @team.motto %></h2>

<h3>Members:</h3>
<br>
<% @superheroes.each.with_index do |hero, index| %>
  <div class="hero">
    <%=index + 1%>
    <ul>
    <li><b>Hero Name: <%= hero.name %></b></li><br>
    <li>Hero Power: <%= hero.power %></li><br>
    <li>Hero Biography: <%= hero.bio %></li>
  </ul>
  </div><br>
<% end %>
